
CREATE KEYSPACE coafstatim WITH replication = {'class': 'NetworkTopologyStrategy', 'DC1': '3', 'DC1_SPARK': '2'}  AND durable_writes = true;


CREATE TABLE coafstatim.application (
    appid bigint PRIMARY KEY,
    advtsgsrccd text,
    appasgnmtaudt set<text>,
    appattrmtrx text,
    appbrsengndata set<text>,
    appcntrtstrc text,
    appcretddt timestamp,
    appcretddtmap_ map<text, text>,
    appcrplcyflexexcpn set<text>,
    appdealstrc text,
    apphighsideflexrespns set<text>,
    appownrtxt text,
    appownrupdtdt timestamp,
    apprecvddt timestamp,
    apprecvddtmap_ map<text, text>,
    apprlexpirydt timestamp,
    appstatbyid int,
    appstatdt timestamp,
    appstatdtmap_ map<text, text>,
    appstatlkupid int,
    appstpln text,
    apptaskaudt set<text>,
    apptaskaudtdtfltr set<timestamp>,
    apptaskvfcn set<text>,
    apptypecd text,
    appupdtbyid int,
    appupdtdt timestamp,
    appupdtdtmap_ map<text, text>,
    asgndprodid int,
    asmid int,
    audtdetl set<text>,
    banknum int,
    brchid bigint,
    brchnum int,
    bumpingficoscore int,
    bustypeid int,
    buygprirtscorenum int,
    calbktypecd text,
    capstappstatcd text,
    chnghist map<text, frozen<map<timestamp, text>>>,
    chnlid int,
    cmtmentcd text,
    cmtmentlndrlkupid int,
    cmtmentreaslkupid int,
    cmttednm text,
    cntrtcretddt timestamp,
    cntrtdt timestamp,
    cntrtnumtxt text,
    cntrtrecvddt timestamp,
    cntrtrecvddtmap_ map<text, text>,
    cntrtupdtbyid int,
    cntrtupdtdt timestamp,
    cust text,
    cust_empmt set<text>,
    dcsnbyid int,
    dcsndt timestamp,
    dcsnstatlkupid int,
    dlrcmtmentcretdbyid int,
    dlrcmtmentcretddt timestamp,
    dlrcmtmentupdtdbyid int,
    dlrcmtmentupdtdt timestamp,
    dlrshporgid bigint,
    docrvisncd text,
    etlbtchids set<int>,
    etldt timestamp,
    etldtmap_ map<text, text>,
    fundddt timestamp,
    fundddtmap_ map<text, text>,
    fundgcomppnts set<text>,
    fundgprirtscorenum int,
    gorcd text,
    hrflag boolean,
    latechrgstatlkupid int,
    latefeeshawcd text,
    loanbrdgdt timestamp,
    loancustnumtxt text,
    loannum int,
    lobid int,
    maxappasgnmtaudtdt timestamp,
    maxapptaskaudtcretddt timestamp,
    maxaudtcretddt timestamp,
    memaexcpn boolean,
    note set<text>,
    notecretddtfltr timestamp,
    ofrinvlddt timestamp,
    ownrtypecd text,
    pricgsid int,
    procstlkupid int,
    rmid int,
    rsrvnnum int,
    rtrnddt timestamp,
    salesasocid int,
    sbmtternm text,
    solitationid int,
    solr_query text,
    strgynum int,
    tpmvendrappid text,
    turnaroundtmnum int,
    uasgdprodid int,
    veh text,
    wrdrndt timestamp
) WITH bloom_filter_fp_chance = 0.01
    AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
    AND comment = 'Application table that has task, notes, assignment audit and audit details'
    AND compaction = {'class': 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
    AND compression = {'sstable_compression': 'org.apache.cassandra.io.compress.LZ4Compressor'}
    AND dclocal_read_repair_chance = 0.1
    AND default_time_to_live = 0
    AND gc_grace_seconds = 172800
    AND max_index_interval = 2048
    AND memtable_flush_period_in_ms = 0
    AND min_index_interval = 128
    AND read_repair_chance = 0.0
    AND speculative_retry = '99.0PERCENTILE';
