CREATE KEYSPACE "TestKS" WITH replication = {'class': 'NetworkTopologyStrategy', 'DC1': '3'}  AND durable_writes = false;

use "TestKS";

CREATE TYPE card_details (
   num int,
   pin int,
   name text,
   cvv int,
   phone set<int>
   );
  
CREATE TABLE cyclist_mv (cid UUID PRIMARY KEY, name text, age int, birthday date, country text, card card_details);

CREATE MATERIALIZED VIEW cyclist_by_age 
AS SELECT age, birthday, name, country 
	FROM cyclist_mv 
	WHERE age IS NOT NULL AND cid IS NOT NULL 
	PRIMARY KEY (age, cid);

CREATE MATERIALIZED VIEW cyclist_by_country 
AS SELECT age,birthday, name, country, card 
	FROM cyclist_mv 
	WHERE country IS NOT NULL AND cid IS NOT NULL
	PRIMARY KEY (country, cid);

CREATE KEYSPACE "push" WITH replication = {'class': 'NetworkTopologyStrategy', 'DC1': '3'}  AND durable_writes = false;

use "push";
  
CREATE TABLE push_device (cid UUID PRIMARY KEY, name text, age int, birthday date, country text);
